{
    "name": "mcfedr/json-form",
    "description": "A couple of helper files for handling json with symfony forms",
    "keywords": ["json", "forms", "symfony"],
    "license": "MIT",
    "authors": [
        {
            "name": "Fred Cox",
            "email": "mcfedr@gmail.com"
        }
    ],
    "type": "symfony-bundle",
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": { "Mcfedr\\JsonFormBundle\\": "src/Mcfedr/JsonFormBundle" }
    },
    "autoload-dev": {
        "psr-4": { "Mcfedr\\JsonFormBundle\\": "tests/Mcfedr/JsonFormBundle" },
        "classmap": ["tests/TestKernel.php"]
    },
    "require": {
        "php": ">=5.5",
        "symfony/framework-bundle": "^3.0|^4.0",
        "symfony/form": "^3.0|^4.0",
        "symfony/validator": "^3.0|^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0|^5.3",
        "symfony/monolog-bundle": "^3.0|^4.0",
        "sensio/framework-extra-bundle": "^3.0|^4.0|^5.0",
        "friendsofphp/php-cs-fixer": "^2.1",
        "symfony/browser-kit": "^3.0|^4.0"
    }
}
