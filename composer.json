{
    "name": "mcfedr/json-form",
    "description": "A couple of helper files for handling json with symfony forms",
    "keywords": ["json", "forms", "symfony"],
    "license": "MIT",
    "authors": [
        {
            "name": "Fred Cox",
            "email": "mcfedr@gmail.com"
        }
    ],
    "type": "symfony-bundle",
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": { "Mcfedr\\JsonFormBundle\\": "src/Mcfedr/JsonFormBundle" }
    },
    "autoload-dev": {
        "psr-4": { "Mcfedr\\JsonFormBundle\\": "tests/Mcfedr/JsonFormBundle" },
        "classmap": ["tests/TestKernel.php"]
    },
    "require": {
        "php": ">=7.2.5",
        "symfony/framework-bundle": "^4.0|^5",
        "symfony/form": "^4|^5",
        "symfony/validator": "^4.0|^5",
        "psr/container": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8",
        "symfony/monolog-bundle": "^3.4.0",
        "friendsofphp/php-cs-fixer": "^2.1",
        "symfony/browser-kit": "^4|^5",
        "phpstan/phpstan-phpunit": "^0.12",
        "phpstan/phpstan-symfony": "^0.12",
        "phpstan/phpstan-doctrine": "^0.12",
        "symfony/phpunit-bridge": "*",
        "symfony/yaml": "^4|^5"
    }
}
